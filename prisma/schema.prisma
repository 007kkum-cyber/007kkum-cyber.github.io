generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x", "rhel-openssl-3.0.x"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}

model Story {
  id        String   @id @default(cuid())
  title     String
  content   String
  createdAt DateTime @default(now())
}
